{
  "name": "fdsn-service-check",
  "version": "1.0.0-SNAPSHOT",
  "description": "Javascript library for testing FDSN web services.",
  "main": "lib/index.js",
  "dependencies": {
    "seisplotjs": ">= 1.0.1-SNAPSHOT"
  },
  "scripts": {
    "lint": "eslint 'src/*.js' ",
    "test": "echo \"Error: no test specified\" && exit 1",
    "compile": "babel --presets es2015 -d lib/ src/",
    "standalone": "browserify src/*.js --standalone servicecheck -o servicecheck_${npm_package_version}_standalone.js -t [ babelify --presets [ es2015 ] ]",
    "prepare": "npm run compile"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/crotwell/fdsn-service-check.git"
  },
  "keywords": [
    "fdsn",
    "fdsnws",
    "seismology",
    "miniseed",
    "quakeml",
    "stationxml"
  ],
  "author": "Philip Crotwell <crotwell@seis.sc.edu>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/crotwell/fdsn-service-check/issues"
  },
  "homepage": "https://github.com/crotwell/fdsn-service-check",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babelify": "^7.3.0",
    "eslint": "^4.4.1",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "qunit": "^1.0.0"
  }
}
