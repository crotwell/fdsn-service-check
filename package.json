{
  "name": "fdsn-service-check",
  "version": "2.0.0-alpha.1",
  "description": "Javascript library for testing FDSN web services.",
  "main": "lib/index.js",
  "dependencies": {
    "eslint-plugin-flowtype": "^3.9.0",
    "eslint-plugin-node": "^9.0.1",
    "seisplotjs": "^2.0.0"
  },
  "scripts": {
    "lint": "eslint 'src/**/*.js' ",
    "test": "echo \"Error: no test specified\" && exit 1",
    "compile": "babel -d lib/ src/",
    "standalone": "browserify src/*.js --standalone servicecheck -o servicecheck_${npm_package_version}_standalone.js -t [ babelify --presets [ @babel/preset-env ] ]",
    "prepare": "npm run compile"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/crotwell/fdsn-service-check.git"
  },
  "keywords": [
    "fdsn",
    "fdsnws",
    "seismology",
    "miniseed",
    "quakeml",
    "stationxml"
  ],
  "author": "Philip Crotwell <crotwell@seis.sc.edu>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/crotwell/fdsn-service-check/issues"
  },
  "homepage": "https://github.com/crotwell/fdsn-service-check",
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "babelify": "^10.0.0",
    "browserify": "^16.2.3",
    "eslint": "^5.16.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-standard": "^4.0.0"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "browsers": [
              "last 2 versions"
            ],
            "node": "current"
          }
        }
      ]
    ]
  }
}
